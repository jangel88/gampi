# ---------------------------------------------------------------------- 
CC      = cc
CFLAGS	= 

# ---------------------------------------------------------------------- 
.SUFFIXES:	.o .c

.c.o:
	$(CC) $(CFLAGS) -c $<
 
# ---------------------------------------------------------------------- 
TESTEXE = test.exe
TESTOBJ = titan_node_info.o

$(TESTEXE):	$(TESTOBJ)
	$(CC) $(CFLAGS) -o $(TESTEXE) $(TESTOBJ)

# ---------------------------------------------------------------------- 

clean:
	find . -name "*.o" -exec rm {} \; 

